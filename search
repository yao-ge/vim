#!/bin/bash

find ./ -name \*.py -o -name \*.sh -o -name \*.c -o -name \*.cpp -o \
		 -name \*.h -o -name \*.p4 -o -name \*.cu -o -name \*.cc | xargs grep "$*" -n --color
